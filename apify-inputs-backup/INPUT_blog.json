{
    "algoliaAppId": "R7KOJZJG1V",
    "algoliaIndexName": "prod_APIFY_HELP",
    "crawlerName": "blogCrawler",
    "startUrls": [
        { "url": "https://blog.apify.com/" }
    ],
    "selectors": [
        {
            "key": "title",
